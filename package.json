{
  "name": "demographics-client",
  "version": "0.0.0",
  "description": "An experimental client-side app for demographics (project stats API)",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public",
    "dev": "npm run clean && NODE_ENV=development webpack-dev-server --progress --hot",
    "postinstall": "cp client/config/example.js client/config/production.js && cp client/config/example.js client/config/development.js",
    "start": "npm run clean && NODE_ENV=production webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmodulus/demographics-client.git"
  },
  "author": "Fran Varney <fran@modulus.io>",
  "bugs": {
    "url": "https://github.com/onmodulus/demographics-client/issues"
  },
  "homepage": "https://github.com/onmodulus/demographics-client#readme",
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-loader": "6.2.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "file-loader": "0.8.5",
    "history": "1.17.0",
    "html-webpack-plugin": "1.7.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-router": "1.0.3",
    "source-map-loader": "0.1.5",
    "webpack": "1.12.11",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.0"
  }
}
